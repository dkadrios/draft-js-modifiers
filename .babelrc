{
  "plugins": ["@babel/plugin-transform-flow-strip-types"],
  "env": {
    "commonjs": {
      "presets": ["@babel/preset-es2017", "@babel/preset-es2016", "@babel/preset-es2015"],
    },
    "es":{
      "presets": ["@babel/preset-es2017", "@babel/preset-es2016", ["@babel/preset-es2015", { "modules": false }]],
    },
    "test": {
      "presets": ["@babel/preset-es2017", "@babel/preset-es2016", "@babel/preset-es2015"]
    }
  }
}
